{
	"servers": {
		"mcp-index-server": {
			"type": "stdio",
			"command": "node",
			"cwd": "C:/path/to/mcp-index-server", // replace with your local clone path
			"args": [
				"scripts/mcp-dev-launch.cjs",
				"--dashboard",
				"--dashboard-port=3210"
			],
			"env": {
				"MCP_LOG_VERBOSE": "1",
				"MCP_ENABLE_MUTATION": "1"
			}
		}
	},
	"inputs": []
}
